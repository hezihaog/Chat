<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/base_white"
    android:orientation="vertical">

    <View
        style="@style/base_divider"
        android:layout_width="match_parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.zh.android.base.widget.iconfont.IconFontTextView
            android:id="@+id/go_back"
            style="@style/base_web_navigation_item"
            android:text="@string/base_web_navigation_bar_go_back"
            android:textColor="@color/base_gray"
            app:ift_typeface="flutter" />

        <com.zh.android.base.widget.iconfont.IconFontTextView
            android:id="@+id/forward"
            style="@style/base_web_navigation_item"
            android:text="@string/base_web_navigation_bar_forward"
            android:textColor="@color/base_gray"
            app:ift_typeface="flutter" />

        <com.zh.android.base.widget.iconfont.IconFontTextView
            android:id="@+id/refresh"
            style="@style/base_web_navigation_item"
            android:text="@string/base_web_navigation_bar_refresh"
            app:ift_typeface="flutter" />

        <com.zh.android.base.widget.iconfont.IconFontTextView
            android:id="@+id/collect"
            style="@style/base_web_navigation_item"
            android:text="@string/base_web_navigation_bar_is_collect"
            app:ift_typeface="flutter" />
    </LinearLayout>
</LinearLayout>